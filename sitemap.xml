---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>https://www.pwindows.qzz.io/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
  </url>
  {% for page in site.pages %}
    {% if page.sitemap != false and page.url != "/" and page.url contains ".css" == false and page.url contains ".js" == false %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
      <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
    </url>
    {% endif %}
  {% endfor %}
  {% for article in site.articles %}
    <url>
      <loc>{{ site.url }}{{ article.url }}</loc>
      <lastmod>{{ article.date | date_to_xmlschema }}</lastmod>
      <changefreq>never</changefreq>
      <priority>0.7</priority>
    </url>
  {% endfor %}
</urlset>
